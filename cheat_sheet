____sorting and array___
#include <stdio.h>
#include <math.h>

void main()
{
    int a[20],b,i;
    printf("enter the number of numbers in your list(max 20):");
    scanf("%d",&b);
    for(i=0;i<b;i++){
        printf("enter the %d number:",i+1);
        scanf("%d",&a[i]);
    }
    for(i=0;i<b;i++){
        printf("\n %d",a[i]);
    }
    printf("are the list of the arrays");
}
