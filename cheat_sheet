_____________using a string__--_
#include <stdio.h>
#include <math.h>

void main()
{
    char c;
    printf("enter the character:");
    scanf("%c",&c);
    if(c=='a'||c=='A'||c=='e'||c=='E'||c=='I'||c=='i'||c=='o'||c=='o'||c=='u'||c=='U'){
        printf("%c is the vowel",c);
    }
    else{
        printf("%c is the consonat",c);
    }
}
